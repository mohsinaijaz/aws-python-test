#!/usr/bin/env groovy

pipeline {
    agent any
    environment {
        AWS_ACCESS_KEY = credentials('JenkinsUser')
        AWS_DEFAULT_REGION = 'us-east-1'
        AWS_ACCESS_KEY_ID = "${JenkinsUser_USR}"
        AWS_SECRET_ACCESS_KEY = "${JenkinsUser_PSW}"
    }
    stages {
        stage('Test AMI') {
            steps {
              sh 'python ec2.py'
            }
        }
    }
}
